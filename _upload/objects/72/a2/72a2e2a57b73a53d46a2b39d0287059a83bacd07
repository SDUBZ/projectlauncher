# Configuration file

general {
    B:"Enable that penguins drop fish (0 - 2 Raw fish)"=false
    B:"Penguins should drop experience?"=true
}


##########################################################################################################
# spawn chances
#--------------------------------------------------------------------------------------------------------#
# Configure penguins spawn weight & min/max group size. Set weight to 0 to disable.
##########################################################################################################

"spawn chances" {

    adelie_penguin {
        I:Max=4
        I:Min=1
        I:Weight=2

        "spawnable biomes" {
            # BiomeDictionary types to exclude [default: [Ljava.lang.String;@49feb427]
            S:Exclude <
                FOREST
                MOUNTAIN
                NETHER
             >

            # BiomeDictionary types to include [default: [Ljava.lang.String;@19215139]
            S:Include <
                SNOWY
             >
        }

    }

}


