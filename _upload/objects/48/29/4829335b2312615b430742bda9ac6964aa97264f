# Configuration file

entityancientgolem {
    # Try to spawn in every biome.  If false, it will use the whitelist in this config  [default: true]
    B:allBiomes=true

    # Base attack, before weapons and buffs [range: 0.0 ~ 100.0, default: 5.0]
    S:baseDamage=5.0

    # Biomes this will spawn into.  Add support for any modded biome here.  Ignored whenever allBiomes is true. https://minecraft.gamepedia.com/Data_values#Biome_IDs [default: [minecraft:plains], [minecraft:forest], [minecraft:swampland], [minecraft:savanna], [minecraft:mesa]]
    S:biomeWhitelist <
        minecraft:plains
        minecraft:forest
        minecraft:swampland
        minecraft:savanna
        minecraft:mesa
     >

    # Base follow range [range: 0 ~ 2, default: 16]
    I:followRange=16

    # Max health of the mob [range: 1 ~ 100, default: 25]
    I:maxHealth=25

    # Biggest spawn group. [range: 0 ~ 500, default: 1]
    I:maxSpawnCount=1

    # Smallest spawn group. [range: 0 ~ 500, default: 1]
    I:minSpawnCount=1

    # Configures the spawning frequency. Higher numbers mean more spawns. [range: 0 ~ 500, default: 15]
    I:weightProbability=7
}


